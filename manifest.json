{
    "name": "Toggl Timer Skin for Gmail",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Changes Toggl Mobile to fit Gmail layout",
    "icons": {
        "128": "icon-128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://m.toggl.com/*", "https://mobile.toggl.com/*"],
            "css": ["styles.css"],
            "all_frames": true
        }
    ]
}
